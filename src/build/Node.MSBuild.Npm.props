<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <NpmVersion Condition="'$(NpmVersion)' == ''"
      >3.5.2</NpmVersion>
    <NpmHomeBase Condition="'$(NpmHomeBase)' == ''"
      >$(USERPROFILE)\.node-msbuild\npm-{0}</NpmHomeBase>
